
{
  "expo": {
    "name": "Afroman TV",
    "slug": "Afroman TV",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/1e10f7f3-1517-4585-aaa3-7e62500446bb.jpeg",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/1e10f7f3-1517-4585-aaa3-7e62500446bb.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.Natively",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/1e10f7f3-1517-4585-aaa3-7e62500446bb.jpeg",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.anonymous.Natively",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "afromantv.app",
              "pathPrefix": "/payment-success"
            },
            {
              "scheme": "afromantv"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/1e10f7f3-1517-4585-aaa3-7e62500446bb.jpeg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "afromantv",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
