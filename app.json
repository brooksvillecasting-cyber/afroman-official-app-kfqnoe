
{
  "expo": {
    "name": "Official Afroman App",
    "slug": "official-afroman-app",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/images/78f09d30-583a-4e97-8a0c-72de573f1869.jpeg",
    "userInterfaceStyle": "automatic",
    "description": "The Official Afroman App - Your complete destination for Grammy-nominated artist Afroman. SHOP: Browse and purchase exclusive official merchandise including premium t-shirts (sizes S-5X) and hoodies (sizes S-5X) with secure Stripe checkout. WATCH: Stream free music videos including the Grammy-nominated hit 'Because I Got High' and 'Crazy Rap (Colt 45)' directly from Afroman's official YouTube channel. Purchase and stream the exclusive movie 'Happily Divorced'. DISCOVER: Explore Afroman's biography, achievements including his Grammy nomination, platinum certifications, 500+ million streams, and his lasting impact on hip-hop culture. FEATURES: Fast-loading interface, smooth video playback, shopping cart with size selection and dynamic pricing, secure payment processing, and a sleek money-green and black design. Download now to access official Afroman content, shop exclusive merchandise, and enjoy free music videos!",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/78f09d30-583a-4e97-8a0c-72de573f1869.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.afroman.officialapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "youtube.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSIncludesSubdomains": true,
              "NSExceptionRequiresForwardSecrecy": true,
              "NSExceptionMinimumTLSVersion": "TLSv1.2"
            },
            "ytimg.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSIncludesSubdomains": true,
              "NSExceptionRequiresForwardSecrecy": true,
              "NSExceptionMinimumTLSVersion": "TLSv1.2"
            },
            "googlevideo.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSIncludesSubdomains": true,
              "NSExceptionRequiresForwardSecrecy": true,
              "NSExceptionMinimumTLSVersion": "TLSv1.2"
            },
            "stripe.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSIncludesSubdomains": true,
              "NSExceptionRequiresForwardSecrecy": true
            },
            "buy.stripe.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSIncludesSubdomains": true,
              "NSExceptionRequiresForwardSecrecy": true
            }
          }
        },
        "CFBundleAllowMixedLocalizations": true,
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/78f09d30-583a-4e97-8a0c-72de573f1869.jpeg",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.afroman.officialapp",
      "permissions": ["INTERNET", "ACCESS_NETWORK_STATE"],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "afromanapp.com",
              "pathPrefix": "/payment-success"
            },
            {
              "scheme": "afromanapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/78f09d30-583a-4e97-8a0c-72de573f1869.jpeg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser",
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": [],
          "enableGooglePay": false
        }
      ]
    ],
    "scheme": "afromanapp",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
