
{
  "expo": {
    "name": "Official Afroman App",
    "slug": "official-afroman-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/78f09d30-583a-4e97-8a0c-72de573f1869.jpeg",
    "userInterfaceStyle": "automatic",
    "description": "The official Afroman mobile app - Shop exclusive merchandise including t-shirts, hoodies, and movies. Watch free music videos including the Grammy-nominated hit 'Because I Got High' and 'Crazy Rap (Colt 45)'. Learn about Grammy-nominated artist Afroman's achievements, biography, and legacy. Features secure Stripe checkout, fast loading, and a sleek money-green and black design. Download now to access official Afroman content!",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/78f09d30-583a-4e97-8a0c-72de573f1869.jpeg",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.afroman.officialapp",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSAppTransportSecurity": {
          "NSAllowsArbitraryLoads": false,
          "NSExceptionDomains": {
            "youtube.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSIncludesSubdomains": true,
              "NSExceptionRequiresForwardSecrecy": true
            },
            "ytimg.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSIncludesSubdomains": true,
              "NSExceptionRequiresForwardSecrecy": true
            },
            "stripe.com": {
              "NSExceptionAllowsInsecureHTTPLoads": false,
              "NSIncludesSubdomains": true,
              "NSExceptionRequiresForwardSecrecy": true
            }
          }
        }
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/78f09d30-583a-4e97-8a0c-72de573f1869.jpeg",
        "backgroundColor": "#000000"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.afroman.officialapp",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "afromanapp.com",
              "pathPrefix": "/payment-success"
            },
            {
              "scheme": "afromanapp"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ]
    },
    "web": {
      "favicon": "./assets/images/78f09d30-583a-4e97-8a0c-72de573f1869.jpeg",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font",
      "expo-router",
      "expo-web-browser"
    ],
    "scheme": "afromanapp",
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {}
    }
  }
}
